<!-- Dashboard Header Section    -->
@*<script>
    $(document).ready(function () {   
        myMap();
     })
     
    function myMap() {  
        $.ajax({
            type: "post",
            url: "/Location/LocatioList?date=",
            datatype: "json",
            traditional: true,
            success: function (data) {
                var map = new google.maps.Map(document.getElementById('googleMap'), {
                    zoom: 10,
                    center: new google.maps.LatLng(data[0].lat, data[0].log),
                    mapTypeId: google.maps.MapTypeId.ROADMAP
                });

                var infowindow = new google.maps.InfoWindow();

                var marker, i;
                // Get Addres
                for (i = 0; i < data.length; i++) {
                    var address = "";
                    $.ajax({
                        type: "post",
                        url: "/Location/GetAddress?location=" + data[i].lat + "," + data[i].log,
                        data: { stateId: $('#ddlState').val() },
                        datatype: "json",
                        traditional: true,
                        success: function (data) {
                            address = data;
                        }
                    });
                    marker = new google.maps.Marker({
                        position: new google.maps.LatLng(data[i].lat, data[i].log),
                        map: map
                    });
                    google.maps.event.addListener(marker, 'click', (function (marker, i) {
                        return function () {
                            infowindow.setContent('<div class=infowindow> <h3>' + data[i].userName + '</h3>Details:<br><p>Date:' + data[i].date + '</p><p>Time:' + data[i].time + '</p><br><div style="height:auto; width:150px"><p>' + address + '</p></div></div>');
                            infowindow.open(map, marker);
                        }
                    })(marker, i));
                }
            }

        });
    }

</script>*@


