<section class="dashboard-counts no-padding-bottom"style="padding:0px;">
    <div class="container-fluid">
        <div class="row bg-white has-shadow">
            <h3 class="no-margin-bottom"> उपस्थिती तपशील </h3>
            <div class="col-12 bg-white has-shadow" id="map" onload="myMap()" style="width:1200px; height:600px"></div>

