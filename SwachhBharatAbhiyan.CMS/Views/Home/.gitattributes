                        pin.Title = data[i].userName;//usually title of the infobox
                        var address = "";
                        $.ajax({
                            type: "post",
                            url: "/Location/GetAddress?location=" + data[i].lat + "," + data[i].log,
                            data: { stateId: $('#ddlState').val() },
                            datatype: "json",
                            traditional: true,
                            success: function (data) {
                                adddress = data;
                               
                            }
                        });


                        //information you want to display in the infobox
                        pin.Description = address;
                        pinLayer.push(pin); //add pushpin to pinLayer
                        Microsoft.Maps.Events.addHandler(pin, 'click', displayInfobox);
                    }

                    map.entities.push(pinLayer);
                    map.entities.push(infoboxLayer);
                    map.setView({ zoom: 15, center: new Microsoft.Maps.Location(data[0].lat, data[0].log) });
                }
            });

     });
    function displayInfobox(e) 
    {
        pinInfobox.setOptions({title: e.target.Title, description: e.target.Description, visible:true, offset: new Microsoft.Maps.Point(0,25)});
        pinInfobox.setLocation(e.target.getLocation());
    }

    function hideInfobox(e) 
    {
        pinInfobox.setOptions({ visible: false });
    }
  
   
</script>



